            <local:Ponto Title="a"  />
        </x:Arguments>
    </NavigationPage>
    <NavigationPage Title="Registrar" IconImageSource="{StaticResource TabTwo}">
        <x:Arguments>
            <local:P2 Title="Two" />
        </x:Arguments>
    </NavigationPage>
   
</TabbedPage>
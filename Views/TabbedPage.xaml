            android:TabbedPage.ToolbarPlacement="Bottom"
            x:Class="RepMobile.Views.TabbedPage"
            BackgroundColor="LightGray"
            BarTextColor="{StaticResource Black}"
            >
            

    <NavigationPage Title="Registros" IconImageSource="{StaticResource TabOne}">
